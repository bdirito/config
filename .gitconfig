[user]
    email = brian@boulderlabs.com
    name = bdirito
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
[push]
    default = simple
[color]
    ui = true
[init]
	templatedir = ~/.git_templates
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
